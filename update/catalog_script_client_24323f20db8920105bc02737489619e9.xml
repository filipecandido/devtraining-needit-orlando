        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    var ga = new GlideAjax('CepAjax');
    ga.addParam('sysparm_name', 'getAddress');
    ga.addParam('sysparm_cep', newValue);
	
    ga.getXMLAnswer(function(answer) {
        var response = JSON.parse(answer);

        g_form.setValue('neighbor', response.bairro); //used for string field
		g_form.setValue('city', response.localidade);
		g_form.setValue('state', response.uf);
		g_form.setValue('public_place', response.logradouro);

    });
}]]></script>
